<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="content" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ConsulterTicketController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0">
            <Label text="Liste des tickets" />
            <TableView fx:id="tableView" prefHeight="300.0" prefWidth="600.0">
                <columns>
                    <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID" />
                    <TableColumn fx:id="eventColumn" prefWidth="100.0" text="Evenement" />
                    <TableColumn fx:id="prixColumn" prefWidth="100.0" text="Prix" />
                    <TableColumn fx:id="typeColumn" prefWidth="100.0" text="type" />
                    <TableColumn fx:id="etatColumn" prefWidth="100.0" text="etat" />
                    <TableColumn fx:id="nbTicketColumn" prefWidth="100.0" text="Nombre de tickets" />
                </columns>
            </TableView>
        </VBox>
    </children>
</VBox>
